{
  "functions": {
    "api/scrape.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/scrapeKeyword.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/download/[csvId].js": {
      "memory": 512,
      "maxDuration": 5
    }
  },
  "rewrites": [
    {
      "source": "/((?!api|_next|favicon.ico).*)",
      "destination": "/public/index.html"
    }
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}